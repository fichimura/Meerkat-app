{
    "rules": {
        "no-console": "off",
        "linebreak-style": 0,
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "es5",
                "singleQuote": true,
                "printWidth": 120
            }
        ]
    },
    "plugins": [
        "prettier"
    ]
}